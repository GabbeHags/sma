{
  "build": {
    "beforeBuildCommand": "cargo build --bin sma --profile prod && cp ../target/prod/sma.exe ./src-tauri/ && npm run check && npm run build",
    "beforeDevCommand": "cargo build --bin sma && cp ../target/debug/sma.exe ./src-tauri/ && npm run check && npm run dev",
    "beforeBundleCommand": "",
    "devPath": "http://localhost:5173",
    "distDir": "../build"
  },
  "package": {
    "productName": "sma-gui",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true,
        "save": true,
        "message": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.sma",
      "longDescription": "",
      "resources": ["sma.exe"],
      "shortDescription": "",
      "targets": "msi",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "webviewInstallMode": {
          "type": "embedBootstrapper"
        },
        "wix": {
          "license": "../../LICENSE"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "width": 480,
        "resizable": false,
        "title": "SMA"
      }
    ]
  }
}
